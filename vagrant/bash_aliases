alias l='ls -l --sort=extension'
alias prune='git fetch --prune origin'
alias c='clear'
alias findjs='find -name "js" -type d | grep -Pv "(node_modules|staticfiles|static/js|static/js/components)"'
alias pep8='pep8 --ignore=E501,E731,E402 --exclude=node_modules,moodle,moodledata,phpu_moodledata'
alias runserver='workon plugins_py2; /vagrant/manage.py runserver 0.0.0.0:8000'
